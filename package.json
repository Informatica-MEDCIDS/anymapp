{
  "name": "anymapp",
  "version": "1.0.0",
  "description": "Anonymous Digital Twin Human-App Interactions",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www.js",
    "debug": "concurrently \"nodemon --inspect=0.0.0.0:9229 ./bin/www.js\" \"webpack --mode=development --node-env=development --watch\"",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development --node-env=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**"
    ],
    "watch": [
      "app.js",
      "routes",
      "public",
      "views",
      "bin/www.js",
      "public/anymapp.js"
    ],
    "delay": 2500
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informatica-MEDCIDS/anymapp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Informatica-MEDCIDS/anymapp/issues"
  },
  "homepage": "https://github.com/Informatica-MEDCIDS/anymapp#readme",
  "dependencies": {
    "bunyan": "^1.8.15",
    "cjs-to-es6": "^2.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "html-express-js": "^1.1.1",
    "http-errors": ">=2.0.0",
    "jstransformer-markdown-it": "^3.0.0",
    "mongodb": "^4.13.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "strip-bom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@webpack-cli/generators": "^3.0.1",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.6.0",
    "javascript-obfuscator": "^4.0.0",
    "nodemon": "^2.0.20",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-obfuscator": "^3.5.1"
  }
}
